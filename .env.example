# Dev Pro Agents - Environment Configuration
# ==========================================
# Complete environment variable reference for the multi-agent orchestration system.
# Copy this file to .env and fill in your actual values.

# === CORE API KEYS (Required) ===
# These API keys are required for the orchestration system to function

# OpenAI API Key - Required for LLM operations
OPENAI_API_KEY=sk-proj-your-openai-api-key-here

# OpenRouter API Key - Required for specialized agent operations
OPENROUTER_API_KEY=sk-or-your-openrouter-api-key-here

# Groq API Key - Optional high-speed LLM provider
GROQ_API_KEY=gsk_your-groq-api-key-here

# Exa API Key - Required for web research and search capabilities
EXA_API_KEY=your-exa-api-key-here

# Firecrawl API Key - Required for web scraping and crawling
FIRECRAWL_API_KEY=fc-your-firecrawl-api-key-here

# === DATABASE CONFIGURATION ===
# Task management and orchestration database
DATABASE_URL=sqlite:///dev_pro_agents.db

# Task manager database path (relative to project root)
ORCHESTRATION__DATABASE__IMPLEMENTATION_TRACKER_PATH=src/database/implementation_tracker.db

# Database connection pool settings
ORCHESTRATION__DATABASE__POOL_SIZE=10
ORCHESTRATION__DATABASE__POOL_TIMEOUT=30
ORCHESTRATION__DATABASE__ECHO_SQL=false

# === API CLIENT CONFIGURATION ===
# Fine-tune API client behavior and timeouts

# OpenAI Configuration
ORCHESTRATION__OPENAI__BASE_URL=https://api.openai.com/v1
ORCHESTRATION__OPENAI__MODEL=gpt-4o-mini
ORCHESTRATION__OPENAI__TEMPERATURE=0.1
ORCHESTRATION__OPENAI__MAX_TOKENS=4000
ORCHESTRATION__OPENAI__TIMEOUT_SECONDS=60
ORCHESTRATION__OPENAI__MAX_RETRIES=3

# OpenRouter Configuration
ORCHESTRATION__OPENROUTER__BASE_URL=https://openrouter.ai/api/v1
ORCHESTRATION__OPENROUTER__MODEL=openrouter/auto
ORCHESTRATION__OPENROUTER__TEMPERATURE=0.2
ORCHESTRATION__OPENROUTER__MAX_TOKENS=4000
ORCHESTRATION__OPENROUTER__TIMEOUT_SECONDS=90
ORCHESTRATION__OPENROUTER__MAX_RETRIES=3

# Groq Configuration
ORCHESTRATION__GROQ__BASE_URL=https://api.groq.com/openai/v1
ORCHESTRATION__GROQ__MODEL=llama-3.1-70b-versatile
ORCHESTRATION__GROQ__TEMPERATURE=0.1
ORCHESTRATION__GROQ__MAX_TOKENS=4000
ORCHESTRATION__GROQ__TIMEOUT_SECONDS=30
ORCHESTRATION__GROQ__MAX_RETRIES=3

# Exa Configuration
ORCHESTRATION__EXA__BASE_URL=https://api.exa.ai
ORCHESTRATION__EXA__SEARCH_TYPE=auto
ORCHESTRATION__EXA__NUM_RESULTS=10
ORCHESTRATION__EXA__INCLUDE_TEXT=false
ORCHESTRATION__EXA__INCLUDE_HIGHLIGHTS=true
ORCHESTRATION__EXA__INCLUDE_SUMMARY=false
ORCHESTRATION__EXA__TIMEOUT_SECONDS=60
ORCHESTRATION__EXA__MAX_RETRIES=3

# Firecrawl Configuration
ORCHESTRATION__FIRECRAWL__BASE_URL=https://api.firecrawl.dev/v1
ORCHESTRATION__FIRECRAWL__TIMEOUT_SECONDS=120
ORCHESTRATION__FIRECRAWL__DEFAULT_FORMATS=["markdown"]
ORCHESTRATION__FIRECRAWL__ONLY_MAIN_CONTENT=true
ORCHESTRATION__FIRECRAWL__CRAWL_LIMIT=100
ORCHESTRATION__FIRECRAWL__MAX_WAIT_TIME=600
ORCHESTRATION__FIRECRAWL__POLL_INTERVAL=10
ORCHESTRATION__FIRECRAWL__MAX_RETRIES=3

# === AGENT CONFIGURATION ===
# Agent-specific settings for different agent types

# Temperature settings for different agent types
ORCHESTRATION__AGENTS__RESEARCH_TEMPERATURE=0.1
ORCHESTRATION__AGENTS__CODING_TEMPERATURE=0.2
ORCHESTRATION__AGENTS__TESTING_TEMPERATURE=0.1
ORCHESTRATION__AGENTS__DOCUMENTATION_TEMPERATURE=0.3

# Agent execution limits
ORCHESTRATION__AGENTS__MAX_EXECUTION_TIME_MINUTES=30
ORCHESTRATION__AGENTS__MAX_ITERATIONS=5
ORCHESTRATION__AGENTS__MINIMUM_CONFIDENCE_SCORE=0.7
ORCHESTRATION__AGENTS__MAX_CONCURRENT_TASKS=3

# === SUPERVISOR CONFIGURATION ===
# LangGraph supervisor settings

ORCHESTRATION__SUPERVISOR__MODEL=openrouter/auto
ORCHESTRATION__SUPERVISOR__TEMPERATURE=0.2
ORCHESTRATION__SUPERVISOR__MAX_PARALLEL_AGENTS=5
ORCHESTRATION__SUPERVISOR__TASK_ASSIGNMENT_STRATEGY=load_balanced
ORCHESTRATION__SUPERVISOR__SUPERVISION_INTERVAL_SECONDS=30
ORCHESTRATION__SUPERVISOR__AUTO_RETRY_FAILED_TASKS=true
ORCHESTRATION__SUPERVISOR__MAX_RETRY_ATTEMPTS=3

# === PERFORMANCE CONFIGURATION ===
# System-wide performance and execution settings

# Parallel execution limits
ORCHESTRATION__PARALLEL_EXECUTION_LIMIT=5
ORCHESTRATION__BATCH_SIZE=10
ORCHESTRATION__TASK_TIMEOUT_MINUTES=60

# Memory and resource limits
ORCHESTRATION__MAX_MEMORY_MB=2048
ORCHESTRATION__MAX_CPU_PERCENT=80

# === DEVELOPMENT & DEBUGGING ===
# Development-specific configuration

# Enable debug mode and enhanced logging
ORCHESTRATION__DEBUG_MODE=false
ORCHESTRATION__LOG_LEVEL=INFO

# Rich console output settings
ORCHESTRATION__RICH_CONSOLE=true
ORCHESTRATION__RICH_TRACEBACKS=true

# Configuration metadata
ORCHESTRATION__CONFIG_VERSION=1.0.0

# === FEATURE FLAGS ===
# Enable/disable specific system features

# Enable workflow checkpointing (recommended for production)
USE_CHECKPOINTING=true

# Enable agent health monitoring
ENABLE_HEALTH_MONITORING=true

# Enable performance metrics collection
ENABLE_METRICS=true

# Enable task dependency validation
ENABLE_DEPENDENCY_VALIDATION=true

